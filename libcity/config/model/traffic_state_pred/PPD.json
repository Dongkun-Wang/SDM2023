{

  "scaler": "standard",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "spatial_embed_dim": 48,
  "temporal_embed_dim": 96,
  "spatial_num_layers": 4,
  "temporal_num_layers": 4,

  "dropout": 0.1,
  "distill": "soft",

  "num_heads": 4,
  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "steplr",
  "lr_decay_ratio": 0.9,
  "clip_grad_norm": false,
  "use_early_stop": true
}
